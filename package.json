{
  "name": "dicekeys-webapp",
  "version": "0.0.1",
  "description": "DiceKeys App",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dicekeys/dicekeys-webapp.git"
  },
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build-app": "parcel build src/index.html --no-minify --out-dir dist-app/",
    "build-pwmgr": "parcel build src/pwmgr.html --no-minify --out-dir dist-pwmgr/ --out-file index.html",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "keywords": [],
  "author": "Stuart Schechter <stuart@dicekeys.com>",
  "license": "UNLICENSED. All rights reserved",
  "dependencies": {
    "@dicekeys/read-dicekey-js": "^0.0.2",
    "@dicekeys/seeded-crypto-js": "^0.1.16"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "devDependencies": {
    "@types/core-js": "^2.5.3",
    "@types/emscripten": "^1.39.4",
    "@types/jest": "^25.2.2",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^14.0.1",
    "canvas": "^2.6.1",
    "core-js": "^3.6.5",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^26.0.0",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.10.0",
    "typescript": "^3.9.2"
  }
}
